{
  "parser": "@typescript-eslint/parser",
  "extends": [
      "next/core-web-vitals",
      "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
  },
  "rules": {
      "@typescript-eslint/no-explicit-any": "warn",
      "react/react-in-jsx-scope": "off" 
  },
  "settings": {
      "react": {
          "version": "detect"
      }
  }
}